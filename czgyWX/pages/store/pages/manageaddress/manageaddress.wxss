page{
  font-family: "微软雅黑";
}
.item{
  width: 100%;
  height:100px;
  background-color: #fff; 
  border-bottom: 8px solid rgb(245, 246, 247);
}
.item_content{
  margin:0 0 0 10px;
}

.item_info{
padding-left: 6px;
padding-top: 6px;
}

.item .item_content .item_name{
  float: left;
  font-size: 14px;
}
.item .item_content .item_number{
    font-size: 14px;
    margin-left: 100px;
}

.item_address{
  font-size: 14px;
  padding-left: 4px;
  padding-top: 4px;
  padding-bottom: 4px;
  color: #bbb;
  border-bottom: 1px solid #bbb;
}


/* 第三列 */
.item_all{
  display: flex;
  justify-content: space-between;
}

/* 默认地址 */
.item_icon{
 margin: 6px;
 width: 30%;
}

.item_icon_img{
  width: 20px;
  height: 20px;
  float: left;
  margin: 6px;
}
.item_icon_text{
  display: block;
  color: #bbb;
  font-size: 14px;
  padding-top: 6px;
}



/* 编辑 */
.item1{
  display: flex;
  float: right;
}
.item_editor{
    margin: 6px;
}
.item_editor_img{
  width: 20px;
  height: 20px;
  margin:6px 0 6px 0;
  float: left;
}
.item_editor_text{
  font-size: 14px;
  color: #bbb;
  display: block;
  float: right;
  padding-top: 6px;
  padding-left: 4px;
}



.item_del{
    margin: 6px;
}
.item_del_img{
  width: 20px;
  height: 20px;
   margin:6px 0 6px 0;
  float: left;

}
.item_del_text{
  font-size: 14px;
  color: #bbb;
  display: block;
  float: right;
  padding-top: 6px;
  padding-left: 4px;
}



/* 底部添加按钮 */
.add {
    position: fixed;
    width: 100%;
    background-color: #FFF;
    bottom: 0;
    border-bottom: 1px solid #E5E5E5;
    font-size: 18px;
}
.addAddress {
    padding: 10px;
    height: 20px;
    line-height: 20px;
    width: 100%;
    border-radius: 5px; 
    background-color: rgb(230,67,64);
  }
.addAddress text{
  display: block;
  text-align: center;
  color: #fff;
}